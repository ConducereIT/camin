generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "linux-arm64-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model events {
  id          Int      @id @default(autoincrement())
  email       String
  title       String
  start_event DateTime
  end_event   DateTime
  calendar_n  String
  phone       String
  camera      String
}

model infoUser {
  id     Int    @id @default(autoincrement())
  userId String @unique
  phone  String
  camera String
}
